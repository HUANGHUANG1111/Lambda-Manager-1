<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\AboutMsg\AboutMsg.csproj" />
    <ProjectReference Include="..\ACE\ACE.csproj" />
    <ProjectReference Include="..\ConfigObjective\ConfigObjective.csproj" />
    <ProjectReference Include="..\ConfigSetting\ConfigSetting.csproj" />
    <ProjectReference Include="..\LambdaCore\LambdaCore.csproj" />
    <ProjectReference Include="..\Solution\Solution.csproj" />
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="del &quot;$(TargetDir)$(AssemblyName).deps.json&quot;" />
  </Target>

</Project>
