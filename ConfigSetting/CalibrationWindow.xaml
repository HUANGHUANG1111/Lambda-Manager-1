        <StackPanel>
            <UniformGrid Columns="5">
                <Button Content="灯光校准" Click="Button_Click"/>
                <Button Content="位移台校准" Click="Button_Click_1"/>
                <Button Content="算法校准" Click="Button_Click_2"/>
