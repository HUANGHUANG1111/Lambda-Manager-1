        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConfigSetting"
        xmlns:Base="clr-namespace:ThemeManager.Controls;assembly=ThemeManager"   
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800" Background="{DynamicResource WindowBackgroundBrush}"  WindowStyle="ToolWindow">
