<UserControl x:Class="Global.UserControls.ProgressBar1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Global.UserControls"
             xmlns:pro="clr-namespace:ThemeManager.Rangeslider;assembly=ThemeManager"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="200">
    <Grid>
        <pro:RangeSlider SlidThumbVis="Hidden" Width="200" Name="pro" Step="1" LowerValue="{Binding  Path=Current,Mode=TwoWay}"  HigherValue="{Binding LoadingMax,Mode=TwoWay}" LowerRangeBackground="#6EA646" RangeBackground="#6EA646" Minimum="{Binding Path= Minimum,Mode=TwoWay}" Maximum="{Binding Path= Maxmum,Mode=TwoWay}" LowerValueChanged="RangeSlider_LowerValueChanged" ></pro:RangeSlider>      
    </Grid>
</UserControl>
